    "@ladle/react": "^5.0.3",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sass": "^1.90.0",
    "tailwindcss": "^3.4.17",
    "tsup": "^8.5.0",
